{
  "name": "joshuaanderton/livewire",
  "description": "Tools for making great SaaS apps with Laravel and Livewire",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.0",
      "lorisleiva/laravel-actions": "^2.4"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.3"
  },
  "autoload": {
      "psr-4": {
          "LivewireKit\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "LivewireKit\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "LivewireKit\\Providers\\ServiceProvider"
          ]
      }
  }
}
