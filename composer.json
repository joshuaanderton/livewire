{
  "name": "tallstackapp/tools",
  "description": "Tools for making wicked-awesome TALL stack apps",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.0",
      "illuminate/support": "^8.67|^9.0",
      "lorisleiva/laravel-actions": "^2.4"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.3"
  },
  "autoload": {
      "psr-4": {
          "TallStackApp\\Tools\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "TallStackApp\\Tools\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "TallStackApp\\Tools\\Providers\\ServiceProvider"
          ]
      }
  }
}
